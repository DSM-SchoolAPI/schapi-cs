<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>School API for C#</Description>
    <Company>DSM SchoolAPI</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/DSM-SchoolAPI/schapi-cs/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/DSM-SchoolAPI/schapi-cs/blob/master/README.md</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DSM-SchoolAPI/schapi-cs</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>School API, School, Meal, Schedule</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AngleSharp" Version="0.9.9.2" />
  </ItemGroup>

</Project>
